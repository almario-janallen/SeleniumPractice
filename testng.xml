<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="SwagLabsSuite" parallel="tests" thread-count="4">
    <test name="ValidLoginTest">
        <classes>
            <class name="tests.LoginTestParallelExecution">
                <methods>
                    <include name="validLoginTest"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="InvalidLoginTest">
        <classes>
            <class name="tests.LoginTestParallelExecution">
                <methods>
                    <include name="invalidLoginTest"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="LockedOutUserTest">
        <classes>
            <class name="tests.LoginTestParallelExecution">
                <methods>
                    <include name="lockedOutUserTest"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="ProblemUserTest">
        <classes>
            <class name="tests.LoginTestParallelExecution">
                <methods>
                    <include name="problemUserTest"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>
